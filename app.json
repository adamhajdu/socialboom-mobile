{
  "expo": {
    "name": "socialboom",
    "slug": "socialboom",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c7bebe"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sigmatech.socialboom"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sigmatech.socialboom",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f12a3e39-6087-46f8-a628-a6fc6fd4f2b5"
      }
    },
    "owner": "sigmatech",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Socialboom to access your camera to take profile picture"
        }
      ]
    ]
  }
}
